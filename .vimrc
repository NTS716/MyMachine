"plugins
execute pathogen#infect()
filetype plugin indent on
map <F2> :NERDTreeToggle<return>

"spacing
set nowrap
set ts=4

"window navigation
map <C-h> <C-w>h
map <C-j> <C-w>j
map <C-k> <C-w>k
map <C-l> <C-w>l

"apperance
set number
syntax enable

"colorscheme stuff
set t_Co=16
set bg=dark
colorscheme solarized
